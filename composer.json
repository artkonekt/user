{
    "name": "konekt/user",
    "description": "User handling module for Laravel applications",
    "type": "library",
    "keywords": ["laravel", "concord", "concord-module", "user"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/user"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-gd": "*",
        "konekt/concord": "^1.5.1",
        "konekt/enum": "~3.0",
        "konekt/address": "^2.0",
        "konekt/laravel-migration-compatibility": "~1.2",
        "doctrine/dbal": "^2.9, !=2.10.3"
    },
    "autoload": {
        "psr-4": { "Konekt\\User\\": "src"}
    },
    "require-dev": {
        "orchestra/testbench": "~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.2",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\User\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
