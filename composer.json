{
    "name": "konekt/user",
    "description": "User handling module for Laravel applications",
    "type": "library",
    "keywords": ["laravel", "concord", "concord-module", "user"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/user"
    },
    "require": {
        "php": "^7.1.3",
        "ext-gd": "*",
        "konekt/concord": "~1.4",
        "konekt/address": "~1.1",
        "konekt/laravel-migration-compatibility": "~1.1",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": { "Konekt\\User\\": "src"}
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "orchestra/testbench": "~3.5|~4.0",
        "phpunit/phpunit": "6.2 - 7.5|~8.0",
        "mockery/mockery": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\User\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
