{
    "name": "konekt/user",
    "description": "User handling module for Laravel applications",
    "type": "library",
    "keywords": ["laravel", "concord", "concord-module", "user"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/user"
    },
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "konekt/concord": "^1.13",
        "konekt/enum": "^4.1",
        "konekt/address": "^3.0",
        "konekt/laravel-migration-compatibility": "^1.4",
        "laravel/framework": "^10.0|^11.0",
        "doctrine/dbal": "^3.5.1|^4.0"
    },
    "autoload": {
        "psr-4": { "Konekt\\User\\": "src"}
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\User\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
