{
    "name": "konekt/user",
    "description": "User handling module for Laravel applications",
    "type": "library",
    "keywords": ["laravel", "concord", "concord-module", "user"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/user"
    },
    "require": {
        "php": ">=7.0.0",
        "konekt/concord": ">=0.9.0",
        "konekt/address": ">=0.9.0"
    },
    "autoload": {
        "psr-4": { "Konekt\\User\\": "src"}
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "orchestra/testbench": "~3.4.2|~3.5",
        "phpunit/phpunit": "~6.2",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\User\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
